{
    "name": "agenter-labs/illuminate-feature-checker",
    "description": "Feature checker for Laravel",
    "keywords": [
        "laravel",
        "auth",
        "checker"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Abdulla",
            "email": "dev@agenter.com",
            "homepage": "https://agenter.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/support": "^9.0",
        "illuminate/contracts": "^9.0",
        "illuminate/cache": "^9.0",
        "illuminate/database": "^9.9"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "laravel/lumen-framework": "^9.0",
        "fakerphp/faker": "^1.19"
    },
    "autoload": {
        "psr-4": {
            "AgenterLab\\FeatureChecker\\": "./src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/",
            "Database\\Factories\\": "tests/database/factories/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "AgenterLab\\FeatureChecker\\FeatureCheckerServiceProvider"
            ]
        }
    },
    "prefer-stable": true
}
